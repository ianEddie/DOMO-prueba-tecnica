---
import ClientsCardOne from './ClientsCardOne.astro'
import ClientsCardTwo from './ClientsCardTwo.astro'
---

<div>
  <ClientsCardOne />
  <ClientsCardTwo />
</div>
<style>
  div {
    z-index: 10;
    width: 100%;
    display: flex;
    flex-wrap: wrap;
    justify-content: center;
    column-gap: 20px;
    row-gap: 20px;
  }
  @media screen and (max-width: 900px) {
    div {
      margin-top: 30px;
    }
  }
</style>
